<%- include('includes/_header') %>
<%- include('includes/_sidebar') %>
<div class="col-8 ">
    <div class="container text-center">
        <div class="row">
            <div class="col-md-5 mb-5 text-light fs-1"> <strong>Mi Biblioteca</strong>
        </div>
                
    </div>
</div>
<div class="vstack gap-3">
    <% for (let cancion of canciones){ %>
        <div class="container">
            <div class="card border-info mb-3 songsLibrary">

                <div class="card-header">
                    <div class="row">
                        <div class="col-8 justify-content-start">
                            <a href="/EliminarCancion/<%= cancion.id %>/<% if (admin==false) { %>Biblioteca<% } %>"
                            class="material-icons link-info link-offset-3 link-underline-opacity-10">
                                delete
                            </a>
                        </div>
                                


                    </div>
                </div>
                <div class="card-body text-center">
                                <h5 class="card-title">
                                    <%= cancion.nombre %>
                                    <%= admin %>
                                    <%= cancion.id %>
                                </h5>
                                <p class="card-text">
                                    <%= cancion.artista %>
                                </p>
                            </div>
                        </div>
                    </div>
            <%}%>
            </div>
        </div>
        </div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
            crossorigin="anonymous"></script>
        </body>

        </html>