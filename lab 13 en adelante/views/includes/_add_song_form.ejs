<div class="col-8 ">
    <div class="container text-center">
        <div class="row">
            <div class="col-md-5 mb-5 text-light fs-1"> <strong>Nueva canción</strong></div>
        </div>
        <form action="/AgregarCancion" method="POST">
            <input type="hidden" name="_csrf" value="<%= csrfToken %>">
            <div class="row">
                <div class="col-md-5 mb-3 text-light">
                    <div class="input-group mb-3">
                        <span class="input-group-text bg-info-subtle" id="basic-addon1">Nombre</span>
                        <input type="text" class="form-control" name="songname" aria-label="songname"
                            aria-describedby="basic-addon1">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-5 mb-3 text-light">
                    <div class="input-group mb-3">
                        <span class="input-group-text bg-info-subtle" id="basic-addon1">Artista</span>
                        <input type="text" class="form-control" name="artist" aria-label="artist"
                            aria-describedby="basic-addon1">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-5 mb-3 text-light">
                    <div class="mb-3">
                        <button type="submit" class="btn btn-info mb-3">Agregar canción</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
</div>
</div>
</div>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    crossorigin="anonymous"></script>
</body>

</html>