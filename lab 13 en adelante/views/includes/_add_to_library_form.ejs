<% for (let cancion of canciones){ %>
    <div class="col-4 align-items-center bg-tertiary-rgb rounded">
        <div class="card border-info mb-3">
            <div class="card-header text-secondary">
                Agregar canción
            </div>
            <form action="/AgregarCancion/Biblioteca/<%= cancion.id %>" method="POST">
                <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                    <div class="card-body">
                        <h5 class="card-title text-center">¿Agregar '<%= cancion.nombre
                                    %> de <%= cancion.artista %>' a tu biblioteca?</h5>
                        <div class="text-center">
                            <button type="submit" class="btn btn-info">Confirmar</button>
                        </div>

                    </div>
            </form>

                </div>
            </div>
<% } %>